{
  "main": "App.js",
  "aven": {
    "env": "expo",
    "srcDependencies": [
      "navigation-core",
      "navigation-native",
      "navigation-stack"
    ],
    "moduleDependencies": [],
    "envOptions": {
      "app": {
        "name": "playground",
        "description": "This project is really great.",
        "slug": "playground",
        "privacy": "unlisted",
        "sdkVersion": "31.0.0",
        "platforms": [
          "ios",
          "android"
        ],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src-sync/mobile/assets/icon.png",
        "splash": {
          "image": "./src-sync/mobile/assets/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        },
        "updates": {
          "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
          "**/*"
        ],
        "ios": {
          "supportsTablet": true
        }
      }
    }
  }
}
